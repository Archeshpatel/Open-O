<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.PMmodule.model">
	<class name="BedLog" table="bed_log">
		<id
			column="id"
			name="Id"
			type="java.lang.Long"
		>
			<generator class="native" />
		</id>
		
		<property
			column="program_id"
			length="11"
			name="ProgramId"
			not-null="true"
			type="java.lang.Long"
		 />
		 
		<property
			column="provider_no"
			length="11"
			name="ProviderNo"
			not-null="true"
			type="java.lang.String"
		 />

		<property
			column="sheet_id"
			length="11"
			name="SheetId"
			not-null="true"
			type="java.lang.Long"
		 />
		 			
		<property
			column="demographic_no"
			length="11"
			name="DemographicNo"
			not-null="true"
			type="java.lang.Long"
		 />		
		
		<property
			column="time"
			length="50"
			name="Time"
			not-null="true"
			type="string"
		 />
		 
		<property
			column="status"
			length="50"
			name="Status"
			not-null="true"
			type="string"
		 />
		 
		 <property name="dateCreated" column="date_created" not-null="true" type="java.util.Date"/>
		 
		<many-to-one name="Sheet" 
			class="org.caisi.PMmodule.model.BedLogSheet"
			column="sheet_id"
			update="false"
			insert="false"
			lazy="false"
		/>
	</class>
</hibernate-mapping>
