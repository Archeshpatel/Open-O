<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.PMmodule.model">
	<class name="Admission" table="admission">
		<id
			column="am_id"
			name="Id"
			type="java.lang.Long"
		>
			<generator class="native" />
		</id>
		<property
			column="provider_no"
			length="11"
			name="ProviderNo"
			not-null="true"
			type="java.lang.Long"
		 />
		<property
			column="admission_status"
			length="24"
			name="AdmissionStatus"
			not-null="false"
			type="string"
		 />
		<property
			column="client_id"
			length="11"
			name="ClientId"
			not-null="true"
			type="java.lang.Long"
		 />
		<property
			column="admission_date"
			length="19"
			name="AdmissionDate"
			not-null="false"
			type="timestamp"
		 />
		<property
			column="admission_notes"
			length="255"
			name="AdmissionNotes"
			not-null="false"
			type="string"
		 />
		<property
			column="temp_admission"
			length="1"
			name="TempAdmission"
			not-null="false"
			type="string"
		 />
		<property
			column="program_id"
			length="11"
			name="ProgramId"
			not-null="true"
			type="java.lang.Long"
		 />
		<property
			column="temp_admit_discharge"
			length="1"
			name="TempAdmitDischarge"
			not-null="false"
			type="string"
		 />
		<property
			column="discharge_notes"
			length="255"
			name="DischargeNotes"
			not-null="false"
			type="string"
		 />
		<property
			column="discharge_date"
			length="19"
			name="DischargeDate"
			not-null="false"
			type="timestamp"
		 />
		<property
			column="team_id"
			length="11"
			name="TeamId"
			not-null="false"
			type="java.lang.Long"
		 />		
		 <many-to-one name="team" 
			class="org.caisi.PMmodule.model.ProgramTeam"
			column="team_id"
			update="false"
			insert="false"
			lazy="false"
			not-found="ignore"
		/> 
		 <many-to-one name="client" 
			class="org.caisi.PMmodule.model.Demographic"
			column="client_id"
			update="false"
			insert="false"
			lazy="false"
		/>
		
		 <property
		 	name="programName"
		 	type="string"
		 	formula="(select p.name from program p where p.program_id = program_id)"
		 	/>
	</class>
</hibernate-mapping>
