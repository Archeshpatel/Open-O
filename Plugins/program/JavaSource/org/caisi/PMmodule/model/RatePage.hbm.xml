<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.PMmodule.model">
	<class name="RatePage" table="rate_page">
		<id
			column="id"
			name="id"
			type="integer"
		>
			<generator class="native" />
		</id>
		<property
			column="visitors"
			length="11"
			name="visitors"
			not-null="true"
			type="integer"
		 />
		<property
			column="score"
			length="11"
			name="score"
			not-null="true"
			type="integer"
		 />
		<property
			column="page_name"
			length="100"
			name="pageName"
			not-null="true"
			type="string"
		 />
	</class>
</hibernate-mapping>
