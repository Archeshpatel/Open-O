<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.PMmodule.model">
	<class name="Provider" table="provider">
		<id
			column="provider_no"
			name="ProviderNo"
			type="string"
		>
			<generator class="native" />
		</id>
		<property
			column="comments"
			name="Comments"
			not-null="false"
			type="string"
		 />
		<property
			column="phone"
			length="20"
			name="Phone"
			not-null="false"
			type="string"
		 />
		<property
			column="billing_no"
			length="20"
			name="BillingNo"
			not-null="false"
			type="string"
		 />
		<property
			column="work_phone"
			length="50"
			name="WorkPhone"
			not-null="false"
			type="string"
		 />
		<property
			column="address"
			length="40"
			name="Address"
			not-null="false"
			type="string"
		 />
		<property
			column="team"
			length="20"
			name="Team"
			not-null="false"
			type="string"
		 />
		<property
			column="status"
			length="1"
			name="Status"
			not-null="false"
			type="string"
		 />
		<property
			column="last_name"
			length="30"
			name="LastName"
			not-null="true"
			type="string"
		 />
		<property
			column="provider_type"
			length="15"
			name="ProviderType"
			not-null="true"
			type="string"
		 />
		<property
			column="sex"
			length="1"
			name="Sex"
			not-null="true"
			type="string"
		 />
		<property
			column="ohip_no"
			length="20"
			name="OhipNo"
			not-null="false"
			type="string"
		 />
		<property
			column="specialty"
			length="20"
			name="Specialty"
			not-null="true"
			type="string"
		 />
		<property
			column="dob"
			length="10"
			name="Dob"
			not-null="false"
			type="date"
		 />
		<property
			column="hso_no"
			length="10"
			name="HsoNo"
			not-null="false"
			type="string"
		 />
		<property
			column="provider_activity"
			length="3"
			name="ProviderActivity"
			not-null="false"
			type="string"
		 />
		<property
			column="first_name"
			length="30"
			name="FirstName"
			not-null="true"
			type="string"
		 />
		<property
			column="rma_no"
			length="20"
			name="RmaNo"
			not-null="false"
			type="string"
		 />
	</class>
</hibernate-mapping>
