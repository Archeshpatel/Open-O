<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.caisi.casemgmt.model.Allergy" table="allergies">
        <id name="allergyid" column="allergyid" unsaved-value="0">
            <generator class="native"/>
        </id>
        <property name="demographic_no"/>
		<property name="description" column="DESCRIPTION"/>
		<property name="reaction" />
		<property name="entry_date" type="java.util.Date"/>
    </class>
</hibernate-mapping>
