<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.casemgmt.model">
	<class name="ProviderDefaultProgram" table="provider_default_program">
		<id
			column="id"
			name="id"
			type="integer"
		>
			<generator class="native" />
		</id>
		<property
			column="provider_no"
			length="6"
			name="providerNo"
			not-null="true"
			type="string"
		 />
		<property
			column="signnote"
			length="1"
			name="signnote"
			not-null="false"
			type="boolean"
		 />
		<property
			column="program_id"
			length="10"
			name="programId"
			not-null="true"
			type="integer"
		 />
	</class>
</hibernate-mapping>
