<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.casemgmt.model">
	<class name="Messagetbl" table="messagetbl">
		<id
			column="messageid"
			name="messageid"
			type="integer"
		>
			<generator class="native" />
		</id>
		<property
			column="themessage"
			name="themessage"
			not-null="false"
			type="string"
		 />
		<property
			column="pdfattachment"
			name="pdfattachment"
			not-null="false"
			type="binary"
		 />
		<property
			column="attachment"
			name="attachment"
			not-null="false"
			type="string"
		 />
		<property
			column="sentByLocation"
			length="10"
			name="sentByLocation"
			not-null="false"
			type="integer"
		 />
		<property
			column="sentbyNo"
			length="6"
			name="sentbyNo"
			not-null="false"
			type="string"
		 />
		<property
			column="thesubject"
			length="128"
			name="thesubject"
			not-null="false"
			type="string"
		 />
		<property
			column="actionstatus"
			length="2"
			name="actionstatus"
			not-null="false"
			type="string"
		 />
		<property
			column="sentto"
			length="255"
			name="sentto"
			not-null="false"
			type="string"
		 />
		<property
			column="sentby"
			length="62"
			name="sentby"
			not-null="false"
			type="string"
		 />
		<property
			column="theime"
			length="8"
			name="theime"
			not-null="false"
			type="time"
		 />
		<property
			column="thedate"
			length="10"
			name="thedate"
			not-null="false"
			type="date"
		 />
	</class>
</hibernate-mapping>
