    <!-- Login -->
    <action   path="/login"
              type="oscar.login.LoginAction"
              scope="request"
              name="loginForm"
              validate="true"
              input="/login.jsp">
              <forward name="receptionist" path="/receptionist/receptionistcontrol.jsp" redirect="true"/>
              <forward name="provider" path="/provider/providercontrol.jsp" redirect="true"/>
              <forward name="admin" path="/admin/admin.jsp" redirect="true"/>
              <forward name="failure" path="/logout.jsp?login=failed"/>
    </action>

    <!-- Edit oscarMessenger -->
	<action   path="/oscarMessenger/SendMessage"
              type="oscar.oscarMessenger.pageUtil.MsgSendMessageAction"
              name="msgSendMessageForm"
              scope="request"
              input="/oscarMessenger/SendMessage.jsp">
              <forward name="success" path="/oscarMessenger/SentMessage.jsp"/>
    </action>

    <action   path="/oscarMessenger/CreateMessage"
              type="oscar.oscarMessenger.pageUtil.MsgCreateMessageAction"
              name="msgCreateMessageForm"
              scope="request"
              input="/oscarMessenger/CreateMessage.jsp">
              <forward name="success" path="/oscarMessenger/SentMessage.jsp"/>
    </action>

    <action   path="/oscarMessenger/DisplayMessages"
              type="oscar.oscarMessenger.pageUtil.MsgDisplayMessagesAction"
              name="msgDisplayMessagesForm"
              scope="request"
              input="/oscarMessenger/DisplayMessages.jsp">
              <forward name="success" path="/oscarMessenger/DisplayMessages.jsp"/>
    </action>

    <!--NEW START-->
    <action   path="/oscarMessenger/ReDisplayMessages"
              type="oscar.oscarMessenger.pageUtil.MsgReDisplayMessagesAction"
              name="msgDisplayMessagesForm"
              scope="request"
              input="/oscarMessenger/DisplayMessages.jsp">
            <forward name="success" path="/oscarMessenger/DisplayMessages.jsp"/>
    </action>
    <!--NEW END-->

    <action   path="/oscarMessenger/ViewMessage"
              type="oscar.oscarMessenger.pageUtil.MsgViewMessageAction"
              name="msgViewMessageForm"
              scope="request"
              input="/oscarMessenger/DisplayMessages.jsp">
              <forward name="noRights" path="/oscarMessenger/DisplayMessages.jsp"/>
              <forward name="success" path="/oscarMessenger/ViewMessage.jsp"/>
    </action>

    <action   path="/oscarMessenger/HandleMessages"
