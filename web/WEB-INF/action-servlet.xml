<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <!-- Define Struts Actions here -->
	<bean name="/Tickler" class="org.caisi.tickler.web.TicklerAction" singleton="false">
		<property name="ticklerManager">
			<ref bean="ticklerManagerT"/>
		</property>
		<property name="demographicManager">
			<ref bean="demographicManagerT"/>
		</property>
		<property name="providerManager">
			<ref bean="providerManagerT"/>
		</property>
		<property name="preparedTicklerManager">
			<ref bean="preparedTicklerManagerT"/>
		</property>
		<property name="consultationManager">
			<ref bean="consultationManager"/>
		</property>
		<property name="chartManager">
			<ref bean="chartManagerT"/>
		</property>
	</bean>
	
	
	<bean name="/CustomFilter" class="org.caisi.tickler.web.CustomFilterAction" singleton="false">
		<property name="ticklerManager">
			<ref bean="ticklerManagerT"/>
		</property>
		<property name="demographicManager">
			<ref bean="demographicManagerT"/>
		</property>
		<property name="providerManager">
			<ref bean="providerManagerT"/>
		</property>
	</bean>

	<bean name="/UnreadTickler" class="org.caisi.tickler.web.UnreadTicklerAction" singleton="false">
		<property name="ticklerManager">
			<ref bean="ticklerManagerT"/>
		</property>
	</bean>
	
	<bean name="/SystemMessage" class="org.caisi.core.web.SystemMessageAction" singleton="false">
		<property name="systemMessageManager">
			<ref bean="SystemMessageManager"/>
		</property>
	</bean>
	
	<bean name="/CaisiRoleAssigner" class="org.caisi.core.web.RoleAssignerAction" singleton="false">
		<property name="caisiRoleManager">
			<ref bean="CaisiRoleManager"/>
		</property>
	</bean>
	
	<bean name="/CaisiRole" class="org.caisi.core.web.RoleAction" singleton="false">
		<property name="caisiRoleManager">
			<ref bean="CaisiRoleManager"/>
		</property>
	</bean>
</beans>