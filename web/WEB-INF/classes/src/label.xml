<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jasperReport PUBLIC "-//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">

<jasperReport name="label" pageWidth="300" pageHeight="75" columnWidth="200" leftMargin="10" rightMargin="10" 
topMargin="10" bottomMargin="10">
	<reportFont name="sansserif" isDefault="false" fontName="sansserif" size="10" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false" pdfFontName="Helvetica" pdfEncoding="Cp1252" isPdfEmbedded="false"/>
	<parameter name="demo" class="java.lang.String">
	</parameter>
	<queryString><![CDATA[select * from demographic where demographic_no =$P{demo}]]></queryString>
	<field name="last_name" class="java.lang.String">
	</field>
	<field name="chart_no" class="java.lang.String">
	</field>
	<field name="first_name" class="java.lang.String">
	</field>
	<field name="address" class="java.lang.String">
	</field>
	<field name="city" class="java.lang.String">
	</field>
	<field name="province" class="java.lang.String">
	</field>
	<field name="postal" class="java.lang.String">
	</field>
	<field name="phone" class="java.lang.String">
	</field>
	<field name="phone2" class="java.lang.String">
	</field>
	<field name="year_of_birth" class="java.lang.String">
	</field>
	<field name="month_of_birth" class="java.lang.String">
	</field>
	<field name="date_of_birth" class="java.lang.String">
	</field>
	<field name="hin" class="java.lang.String">
	</field>
	<field name="sex" class="java.lang.String">
	</field>
	<title>
		<band height="151">
		</band>
	</title>
	<detail>
		<band>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-148" width="290" 
				height="20"/>
				<textElement>
					<font size="14" isBold="true" isUnderline="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{last_name} + "," + $F{first_name}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-127" width="290" 
				height="16"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["CHART#:" + $F{chart_no}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-110" width="290" 
				height="16"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["HIN:" + $F{hin} + "    " + "SEX:" + $F{sex} + "	   " + "DOB:" +$F{date_of_birth} + "/" + $F{month_of_birth} + "/" + $F{year_of_birth}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-93" width="290" 
				height="16"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{address}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-76" width="290" 
				height="16"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{city} + "," + $F{province} + " " + $F{postal}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-59" width="290" 
				height="16"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["HOME PHONE:" + $F{phone}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="-42" width="290" 
				height="16"/>
				<textElement>
					<font size="10"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["WORK PHONE:" + $F{phone2}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>



	<band height="55">
			<textField>
				<reportElement positionType="Float" mode="Opaque" x="0" y="0" width="330" height="16"/>
				<textElement>
					<font size="12" isBold="true" isUnderline="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{last_name} + "," + $F{first_name}]]></textFieldExpression>
			</textField>
		</band>
