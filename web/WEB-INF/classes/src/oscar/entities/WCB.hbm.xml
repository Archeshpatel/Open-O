<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="oscar.entities">
	<class name="WCB" table="wcb">
		<id column="ID" name="id" type="integer">
			<generator class="native" />
		</id>



<property  name="billing_no"       not-null="true" type="integer" />  
<property  name="demographic_no"    not-null="true" type="integer" />  
<property  name="provider_no"       not-null="true" type="string" />  
<property  name="formCreated" type="java.util.Date" />
<property  name="formEdited" type="java.util.Date" />
<property  name="w_reporttype"      length="1"          not-null="false" type="string" />    
<property  name="bill_amount"       length="10"    not-null="false" type="string" />    
<property  name="w_fname"           length="12"     not-null="false" type="string" />   
<property  name="w_lname"           length="18"      not-null="false" type="string" />  
<property  name="w_mname"           length="1"          not-null="false" type="string" />  
<property  name="w_gender"          length="1"          not-null="false" type="string" /> 
<property  name="w_dob"                                                 type="java.util.Date" />                   
<property  name="w_address"         length="25"     not-null="false" type="string" />   
<property  name="w_city"            length="20"     not-null="false" type="string" />   
<property  name="w_postal"          length="6"     not-null="false" type="string" />   
<property  name="w_area"            length="3"         not-null="false" type="string" />   
<property  name="w_phone"           length="7"      not-null="false" type="string" />   
<property  name="w_phn"             length="12"     not-null="false" type="string" />   
<property  name="w_empname"         length="25"   not-null="false" type="string" />     
<property  name="w_emparea"         length="3"       not-null="false" type="string" />     
<property  name="w_empphone"        length="7"    not-null="false" type="string" />     
<property  name="w_wcbno"           length="25"   not-null="false" type="string" />     
<property  name="w_opaddress"       length="25"   not-null="false" type="string" />     
<property  name="w_opcity"          length="25"   not-null="false" type="string" />     
<property  name="w_rphysician"      length="1"    not-null="false" type="string" />      
<property  name="w_duration"        not-null="true" type="integer" />  
<property  name="w_problem"         length="160"  not-null="false" type="string" />     
<property  name="w_servicedate"     type="java.sql.Timestamp" />            
<property  name="w_diagnosis"       length="120"  not-null="false" type="string" />     
<property  name="w_icd9"            length="5"    not-null="false" type="string" />     
<property  name="w_bp"              length="5"    not-null="false" type="string" />     
<property  name="w_side"            length="2"       not-null="false" type="string" />     
<property  name="w_noi"             length="5"    not-null="false" type="string" />     
<property  name="w_work"            length="1"       not-null="false" type="string" />     
<property  name="w_workdate"                                            type="java.util.Date" />
<property  name="w_clinicinfo"                       not-null="false"   type="string" />     
<property  name="w_capability"      length="1"       not-null="false"   type="string" />     
<property  name="w_capreason"       length="240"     not-null="false"   type="string" />     
<property  name="w_estimate"        length="1"       not-null="false"   type="string" />     
<property  name="w_rehab"           length="1"       not-null="false"   type="string" />     
<property  name="w_rehabtype"       length="1"       not-null="false"   type="string" />     
<property  name="w_wcbadvisor"      length="1"       not-null="false"   type="string" />      
<property  name="w_ftreatment"      length="25"      not-null="false"   type="string" />    
<property  name="w_estimatedate" type="java.sql.Timestamp" /> 
<property  name="w_tofollow"        length="1"       not-null="false"   type="string" />     
<property  name="w_payeeno"         length="10"      not-null="false"   type="string" />     
<property  name="w_pracno"          length="10"      not-null="false"   type="string" />      
<property  name="w_doi"                                                 type="java.util.Date" />
<property  name="status"             length="1"      not-null="false"   type="string" />      
<property  name="w_feeitem"          length="5"      not-null="false" type="string" />       
<property  name="w_extrafeeitem"     length="5"      not-null="false" type="string" />       
<property  name="w_servicelocation"  length="1"      not-null="false" type="string" />
<property  name="formNeeded"         not-null="true" type="integer" />  
                
               

	</class>
</hibernate-mapping>
