<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="oscar.entities">
	<class name="Billing" table="billing">
		<id column="billing_no" name="billingNo" type="integer">
			<generator class="native" />
		</id>

 <property column="clinic_no" name="clinicNo" not-null="true" type="integer" />  
 <property column="demographic_no" name="demographicNo" not-null="true" type="integer" />  
 <property column="provider_no"   name="providerNo" length="6"   not-null="false" type="string" />
 <property column="appointment_no" name="appointmentNo" not-null="true" type="integer" />  
  
 <property column="organization_spec_code"   name="organizationSpecCode" length="5"   not-null="false" type="string" /> 
 <property column="demographic_name"   name="demographicName" length="5"   not-null="false" type="string" /> 
 <property column="hin"   name="hin" length="5"  not-null="false" type="string" /> 
 <property column="update_date"   name="updateDate" length="5"   not-null="false" type="string" /> <!-- Date ? -->
 <property column="update_time"   name="updateTime" length="5"   not-null="false" type="string" /> <!-- Date ? -->
  
 <property column="billing_date"   name="billingDate" length="5"   not-null="false" type="string" /> <!-- Date ? -->
 <property column="billing_time"   name="billingTime" length="5"   not-null="false" type="string" /> <!-- Date ? -->
 <property column="clinic_ref_code"   name="clinicRefCode" length="5"   not-null="false" type="string" /> 
 <property column="content"   name="content" length="5"   not-null="false" type="string" /> 
 <property column="total"   name="total" length="5"   not-null="false" type="string" /> 

 <property column="status"   name="status" length="5"   not-null="false" type="string" /> 
 <property column="dob"   name="dob" length="5"   not-null="false" type="string" /> 
 <property column="visitdate"   name="visitdate" length="5"   not-null="false" type="string" />   <!-- Date ? -->
 <property column="visittype"   name="visittype" length="5"   not-null="false" type="string" />    <!-- Date ? -->
 <property column="provider_ohip_no"   name="providerOhipNo" length="5"   not-null="false" type="string" /> 

 <property column="provider_rma_no"   name="providerRmaNo" length="5"   not-null="false" type="string" /> 
 <property column="apptProvider_no"   name="apptProviderNo" length="5"   not-null="false" type="string" /> 
 <property column="asstProvider_no"   name="asstProviderNo" length="5"   not-null="false" type="string" /> 
 <property column="creator"   name="creator" length="5"   not-null="false" type="string" /> 
 <property column="billingtype"   name="billingtype" length="5"   not-null="false" type="string" /> 


	</class>
</hibernate-mapping>
