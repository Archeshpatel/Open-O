<property column="icbc_claim_no"            length="8" name="icbcClaimNo" not-null="false" type="string" />
<property column="original_claim"           length="20" name="originalClaim" not-null="false" type="string" />
<property column="facility_no"              length="5" name="facilityNo" not-null="false" type="string" />
<property column="facility_sub_no"          length="1" name="facilitySubNo" not-null="false" type="string" />
<property column="filler_claim"             length="58" name="fillerClaim" not-null="false" type="string" />
<property column="oin_insurer_code"         length="2" name="oinInsurerCode" not-null="false" type="string" />
<property column="oin_registration_no"      length="12" name="oinRegistrationNo" not-null="false" type="string" />
<property column="oin_birthdate"            length="8" name="oinBirthdate" not-null="false" type="string" />
<property column="oin_first_name"           length="12" name="oinFirstName" not-null="false" type="string" />
<property column="oin_second_name"          length="1" name="oinSecondName" not-null="false" type="string" />
<property column="oin_surname"              length="18" name="oinSurname" not-null="false" type="string" />
<property column="oin_sex_code"             length="1" name="oinSexCode" not-null="false" type="string" />
<property column="oin_address"              length="25" name="oinAddress" not-null="false" type="string" />
<property column="oin_address2"             length="25" name="oinAddress2" not-null="false" type="string" />
<property column="oin_address3"             length="25" name="oinAddress3" not-null="false" type="string" />
<property column="oin_address4"             length="25" name="oinAddress4" not-null="false" type="string" />
<property column="oin_postalcode"           length="6" name="oinPostalcode" not-null="false" type="string" />
<property column="paymentMethod"            length="1" name="paymentMethod" not-null="false" type="integer" />
<property column="wcb_id"            name="wcbId" not-null="false" type="integer" />

