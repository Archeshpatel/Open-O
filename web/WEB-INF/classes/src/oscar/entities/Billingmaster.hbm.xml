<property column="referral_flag1"           length="1" name="referralFlag1" not-null="false" type="string" />
<property column="referral_no1"             length="5" name="referralNo1" not-null="false" type="string" />
<property column="referral_flag2"           length="1" name="referralFlag2" not-null="false" type="string" />
<property column="referral_no2"             length="5" name="referralNo2" not-null="false" type="string" />
