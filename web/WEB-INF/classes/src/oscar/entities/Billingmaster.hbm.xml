<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="oscar.entities">
	<class name="Billingmaster" table="billingmaster">
		<id column="billingmaster_no" name="billingmasterNo" type="integer">
			<generator class="native" />
		</id>
                
               
<property column="billing_no"  length="10" name="billingNo" not-null="true" type="integer" />  
<property column="createdate" name="createdate" not-null="false" type="date" />
<property column="billingstatus"                 length="1" name="billingstatus" not-null="false" type="string" />
<property column="demographic_no"           length="10" name="demographicNo" not-null="true" type="integer" />  
<property column="appointment_no"           length="10" name="appointmentNo" not-null="true" type="integer" />  
<property column="claimcode"                length="3" name="claimcode" not-null="false" type="string" />
<property column="datacenter"               length="5" name="datacenter" not-null="false" type="string" />
<property column="payee_no"                 length="5" name="payeeNo" not-null="false" type="string" />
<property column="practitioner_no"          length="5" name="practitionerNo" not-null="false" type="string" />
<property column="phn"                      length="10" name="phn" not-null="false" type="string" />
<property column="name_verify"              length="4" name="nameVerify" not-null="false" type="string" />
