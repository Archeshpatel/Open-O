        display_name="Update HIV Test History in PMH"
        guideline=""
        graphable="no"
        value_name="Changed"
    /> 
    

    
    
    <!--
      
      Need to make sure these measurements are below
        -measurement_type="CD4"
        -measurement_type="VLOA"
        ?measurement_type="Hb"
        -measurement_type="ALT"
        +measurement_type="FBS"*
        +measurement_type="TCHL"*
        +measurement_type="LDL"
        +measurement_type="HDL"
        +measurement_type="TG"
        -measurement_type="VB12"
        -measurement_type="FTST"
        -measurement_type="VDRL"
        -measurement_type="TOXP"
        -measurement_type="HpAI"
        -measurement_type="HpBS"
        -measurement_type="HpBA"
        -measurement_type="HpCA"
        -measurement_type="CMVI"
        -measurement_type="G6PD"
        -measurement_type="URBH"
        -measurement_type="USSH"
        -measurement_type="UDUS"
        -measurement_type="UAIP"
        -measurement_type="UHTP"

      -->
      <measurement 
        type="CD4" 
        typeDesc="CD4" 
        typeDisplayName="CD4" 
        measuringInstrc="">
        <validationRule 
            name="Numeric Value: 0 to 2500" 
            maxValue="2500" 
            minValue="0" 
            isNumeric="1"/>
    </measurement> 
    
    <measurement 
        type="WT" 
        typeDesc="Weight" 
        typeDisplayName="Wt" 
        measuringInstrc="in kg (nnn.n) Range:0-300 Interval:3mo.">
        <validationRule 
            name="Numeric Value: 0 to 300" 
            maxValue="300" 
            minValue="0" 
            isNumeric="1"/>
    </measurement>
    
    
    
    <measurement 
        type="VLOA" 
        typeDesc="Viral Load" 
        typeDisplayName="Viral Load" 
        measuringInstrc="">
        <validationRule 
            name="Numeric Value: 0 to 500000" 
            maxValue="500000" 
            minValue="0" 
            isNumeric="1"/>
    </measurement> 
    
   <measurement 
   type="Hb" 
   typeDesc="Hb" 
   typeDisplayName="Hb" 
   measuringInstrc="">
        <validationRule 
        name="Numeric Value: 30 to 200" 
        maxValue="200" 
        minValue="30" 
        isNumeric="1"/>
</measurement>
    
    
    <measurement 
    type="ALT" 
    typeDesc="ALT" 
    typeDisplayName="ALT" 
    measuringInstrc="">
        <validationRule 
        name="Numeric Value: 0 to 300" 
        maxValue="300" 
        minValue="0" 
        isNumeric="1"/>
</measurement>
      
    <measurement 
        type="TCHL" 
        typeDesc="Total Cholestorol" 
        typeDisplayName="Total Cholestorol" 
        measuringInstrc="in mmol/L (nn.n) Range:2.0-12.0">
        <validationRule 
            name="Numeric Value: 2.0 to 12.0" 
            maxValue="12" 
            minValue="2" 
            isNumeric="1"/>
    </measurement> 
    <measurement 
        type="FBS" 
        typeDesc="FBS" 
        typeDisplayName="Fasting Blood Sugar" 
        measuringInstrc="FBS value">
        <validationRule 
            name="Numeric Value: 0 to 100" 
            maxValue="100" 
            minValue="0" 
            isNumeric="1"/>
    </measurement>
    <measurement 
        type="HDL" 
        typeDesc="High Density Lipid" 
        typeDisplayName="HDL" 
        measuringInstrc="in mmol/L (n.n) Range:0.4-4.0">
        <validationRule 
            name="Numeric Value: 0.4 to 4.0" 
            maxValue="4" 
            minValue="0.4" 
            isNumeric="1"/>
    </measurement>
    <measurement 
        type="TG" 
        typeDesc="Triglycerides" 
        typeDisplayName="TG" 
        measuringInstrc="">
        <validationRule 
            name="Numeric Value: 0 to 10" 
            maxValue="10" 
            minValue="0" 
            isNumeric="1"/>
    </measurement>
    <measurement 
        type="LDL" 
        typeDesc="Low Density Lipid" 
        typeDisplayName="LDL" 
        measuringInstrc="in mmol/L (nn.n) Range:0.5-10  Target:lt;2.5 Interval:6-12mo.">
        <validationRule 
            name="Numeric Value: 0.5 to 10" 
            maxValue="10" 
            minValue="0.5" 
            isNumeric="1"/>
    </measurement>
    
    
    
    <measurement 
        type="VB12" 
        typeDesc="Vitamin B12" 
        typeDisplayName="Vit B12" 
        measuringInstrc="">
        <validationRule 
            name="Numeric Value: 0 to 2500" 
            maxValue="2500" 
            minValue="0" 
            isNumeric="1"/>
    </measurement> 
    
   
    
     <measurement 
        type="FTST"      
        typeDesc="Free Testost"      
        typeDisplayName="Free Testost" 
        measuringInstrc="">  
        <validationRule 
            name="Numeric Value: 0 to 2500" 
            maxValue="2500" 
            minValue="0" 
            isNumeric="1"/>  
    </measurement>  
    
   
    
    
    
    <!-- pos neg lab -->
    
    
   
    
    <measurement 
        type="VDRL"      
        typeDesc="VDRL"      
        typeDisplayName="VDRL" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    
    <measurement 
        type="TOXP"      
        typeDesc="Toxoplasma IgG"      
        typeDisplayName="Toxoplasma IgG" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    
    <measurement 
        type="HpAI"      
        typeDesc="Hep A IgG"      
        typeDisplayName="Hep A IgG" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement> 
    
    <measurement 
        type="HpBS"      
        typeDesc="Hep BS Ag"      
        typeDisplayName="Hep BS Ag" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement> 
    <measurement 
        type="HpBA"      
        typeDesc="Hep BS Ab"      
        typeDisplayName="Hep BS Ab" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement> 
     <measurement 
        type="HpCA"      
        typeDesc="Hep C Ab"      
        typeDisplayName="Hep C Ab" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement> 
    <measurement 
        type="CMVI"      
        typeDesc="CMV IgG"      
        typeDisplayName="CMV IgG" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>   
    <measurement 
        type="G6PD"      
        typeDesc="G6PD"      
        typeDisplayName="G6PD" 
        measuringInstrc="Positive">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
       
        
    
    <!-- pos neg lab end -->
    
    
    <!-- update values -->
    <measurement 
        type="URBH"      
        typeDesc="Update Risk Behaviours"      
        typeDisplayName="Update Risk Behaviours" 
        measuringInstrc="Changed">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    <measurement 
        type="USSH"      
        typeDesc="Update Sexual Identity in Social History"      
        typeDisplayName="Update Sexual Identity in Social History" 
        measuringInstrc="Changed">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    <measurement 
        type="UDUS"      
        typeDesc="Update Drug Use"      
        typeDisplayName="Update Drug Use" 
        measuringInstrc="Changed">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    <measurement 
        type="UAIP"      
        typeDesc="Update AIDS defining illness in PMH"      
        typeDisplayName="Update AIDS defining illness in PMH" 
        measuringInstrc="Changed">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    <measurement 
        type="UHTP"      
        typeDesc="Update HIV Test History in PMH"      
        typeDisplayName="Update HIV Test History in PMH" 
        measuringInstrc="Changed">  
        <validationRule
            name="Yes/No"
            regularExp="YES|yes|Yes|Y|NO|no|No|N"/>   
    </measurement>  
    
   
    
    <!-- update values end -->
    
    
</flowsheet>
