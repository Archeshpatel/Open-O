<flowsheet name="hiv" 
           display_name="HIV Flowsheet" 
           ds_rules="hiv.drl" 
           dxcode_triggers="icd9:042" 
           warning_colour="#E00000"
           recommendation_colour="yellow">
    
    <indicator key="LOW" colour="#9999FF"/>
    <indicator key="HIGH" colour="orange"/>
    <indicator key="HIGH 1" colour="#E00000"/>
    
    <item
        measurement_type="CD4"
        display_name="CD4"
        guideline=""
        graphable="yes"
        value_name="CD4"/>
    
    <item
        measurement_type="VLOA"
        display_name="Viral Load"
        guideline=""
        graphable="yes"
        value_name="Viral Load"
    />    
    
    <item
