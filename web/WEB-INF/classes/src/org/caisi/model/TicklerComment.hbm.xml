<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.caisi.model.TicklerComment" table="tickler_comments">
        <id name="id" column="id" unsaved-value="null">
            <generator class="increment"/>
        </id>
		<property name="tickler_no"/>
		<property name="message"/>
		<property name="provider_no"/>
		<property name="update_date" type="java.util.Date"/>
		
		<many-to-one name="provider" 
			class="org.oscarehr.PMmodule.model.Provider"
			column="provider_no"
			update="false"
			insert="false"
			lazy="false"
			not-found="ignore"
		/>
    </class>
</hibernate-mapping>
