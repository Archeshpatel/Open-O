<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.caisi.model.EChart" table="eChart">
        <id name="id" column="eChartId" unsaved-value="0">
        	<generator class="native"/>
		</id>
		<property name="timeStamp" type="java.util.Date"/>
		<property name="demographicNo"/>
		<property name="providerNo"/>
		<property name="subject"/>
		<property name="socialHistory"/>
		<property name="ongoingConcerns"/>
		<property name="familyHistory"/>
		<property name="medicalHistory"/>
		<property name="reminders"/>
		<property name="encounter"/>
    </class>
</hibernate-mapping>
