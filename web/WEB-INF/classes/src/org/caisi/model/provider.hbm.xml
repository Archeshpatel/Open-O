<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.caisi.model.Provider" table="provider">
        <id name="provider_no" column="provider_no" unsaved-value="null">
		</id>
		<property name="firstName" column="first_name"/>
		<property name="lastName" column="last_name"/>
		<property name="provider_type"/>
		
		<many-to-one name="role" 
			class="org.caisi.model.CaisiRole"
			column="provider_no"
			update="false"
			insert="false"
			lazy="false"
			not-found="ignore"
		/>
    </class>
</hibernate-mapping>
