<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.caisi.model">
	<class name="Program" table="program">
		<id
			column="program_id"
			name="id"
			type="integer"
		>
			<generator class="native" />
		</id>
		<property
			column="num_of_members"
			length="4"
			name="numOfMembers"
			not-null="false"
			type="integer"
		 />
		<property
			column="max_allowed"
			length="4"
			name="maxAllowed"
			not-null="false"
			type="integer"
		 />
		<property
			column="type"
			length="50"
			name="type"
			not-null="false"
			type="string"
		 />
		<property
			column="descr"
			length="255"
			name="descr"
			not-null="false"
			type="string"
		 />
		<property
			column="location"
			length="70"
			name="location"
			not-null="false"
			type="string"
		 />
		<property
			column="name"
			length="70"
			name="name"
			not-null="true"
			type="string"
		 />
		<property
			column="agency_id"
			length="11"
			name="agencyId"
			not-null="true"
			type="java.lang.Long"
		 />
	</class>
</hibernate-mapping>
