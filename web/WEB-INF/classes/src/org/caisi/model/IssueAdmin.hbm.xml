<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
    <class name="org.caisi.model.IssueAdmin" table="issue">
         <id name="id" column="issue_id" unsaved-value="0">
             <generator class="increment" />
         </id>
         <property name="code" column="code" not-null="true"/>
         <property name="description" column="description" not-null="true"/>
         <property name="role" column="role" not-null="true"/>
         <property name="update_date" column="update_date" type="java.util.Date"/>
    </class>
</hibernate-mapping>
