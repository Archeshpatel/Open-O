<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.oscarehr.survey.model.oscar">
   	<class name="OscarForm" table="caisi_form">    
        <id name="formId" column="form_id" unsaved-value="0">
            <generator class="native"/>
        </id>
        <property name="description" type="string"/>
        <property name="surveyData" type="text"/>
		<property name="status" type="short"/>
		<property name="version"/>
	</class>
</hibernate-mapping>