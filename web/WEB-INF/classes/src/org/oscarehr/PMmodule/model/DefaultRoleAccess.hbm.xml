<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="DefaultRoleAccess" table="default_role_access">
		<id
			column="id"
			name="id"
			type="java.lang.Long"
		>
			<generator class="native" />
		</id>
		
		<property name="roleId" column="role_id" type="long"/>
		<property name="accessTypeId" column="access_id" type="long"/>
		
		<many-to-one name="caisi_role" 
			class="org.caisi.model.Role"
			column="role_id"
			update="false"
			insert="false"
			lazy="false"
		/>
		
		<many-to-one name="access_type" 
			class="org.oscarehr.PMmodule.model.AccessType"
			column="access_id"
			update="false"
			insert="false"
			lazy="false"
		/>		
	</class>
</hibernate-mapping>
