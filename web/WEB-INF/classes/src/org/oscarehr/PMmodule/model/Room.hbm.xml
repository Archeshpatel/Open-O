<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class
		name="Room"
		table="room">
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="room_id">
			<generator class="native" />
		</id>
		<property
			name="roomTypeId"
			column="room_type_id"
			type="integer"
			not-null="true"
			length="10" />
		<property
			name="programId"
			column="program_id"
			type="integer"
			length="10" />
		<property
			name="name"
			column="name"
			type="string"
			not-null="true"
			length="45" />
		<property
			name="floor"
			column="floor"
			type="string"
			not-null="false"
			length="45" />
		<property
			name="active"
			column="active"
			type="boolean"
			not-null="true" />
	</class>
</hibernate-mapping>