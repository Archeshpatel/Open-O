<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="ProgramProvider" table="program_provider">
		<id
			column="id"
			name="Id"
			type="java.lang.Long"
			unsaved-value="0"
		>
			<generator class="native" />
		</id>

		<property
			column="program_id"
			length="11"
			name="ProgramId"
			not-null="false"
			type="java.lang.Long"
		 />
		 
		<property
			column="provider_no"
			length="11"
			name="ProviderNo"
			not-null="false"
			type="java.lang.Long"
		 />

		<property
			column="role_id"
			length="11"
			name="RoleId"
			not-null="false"
			type="java.lang.Long"
		 />
		 
		<set name="teams" table="program_provider_team" lazy="false">
		    <key column="id"/>
		    <many-to-many class="org.oscarehr.PMmodule.model.ProgramTeam"/>
		</set>
		 
		 <many-to-one name="role" 
			class="org.caisi.model.Role"
			column="role_id"
			update="false"
			insert="false"
			lazy="false"
		/>
		
		 <many-to-one name="provider" 
			class="org.oscarehr.PMmodule.model.Provider"
			column="provider_no"
			update="false"
			insert="false"
			lazy="false"
		/>
	</class>
</hibernate-mapping>
