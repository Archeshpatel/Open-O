<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="Log" table="pmm_log">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property
			column="provider_no"
			name="ProviderNo"
			type="string"
		 />
		<property
			column="dateTime"
			name="DateTime"
			type="timestamp"
		 />
		
		<property
			column="action"
			length="20"
			name="Action"
			not-null="false"
			type="string"
		 />
		<property
			column="contentId"
			length="80"
			name="ContentId"
			not-null="false"
			type="string"
		 />
		<property
			column="content"
			length="80"
			name="Content"
			not-null="true"
			type="string"
		 />
		<property
			column="ip"
			length="30"
			name="Ip"
			not-null="false"
			type="string"
		 />
	</class>
</hibernate-mapping>
