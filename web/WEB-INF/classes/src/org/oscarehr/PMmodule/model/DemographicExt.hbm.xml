<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class
		name="DemographicExt"
		table="demographicExt">
		<id
			column="id"
			name="id"
			type="integer">
			<generator class="native" />
		</id>
		<property
			name="demographicNo"
			column="demographic_no"
			type="integer" />
		<property
			name="providerNo"
			column="provider_no"
			type="integer" />
		<property
			name="key"
			column="key_val"
			type="string" />
		<property
			name="value"
			column="value"
			type="string" />
		<property
			name="dateCreated"
			column="date_time"
			type="date" />
		<property
			name="hidden"
			column="hidden"
			type="boolean" />
	</class>
</hibernate-mapping>