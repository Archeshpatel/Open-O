<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.oscarehr.casemgmt.model.ClientImage" table="client_image">
        <id name="id" column="image_id" unsaved-value="0">
            <generator class="native"/>
        </id>
        
		<property name="demographic_no"/>
		
		<property name="image_type" type="string"/>
		<property name="image_data2" column="image_data" type="string"/>
		<property name="update_date" type="java.util.Date"/>
    </class>
</hibernate-mapping>
