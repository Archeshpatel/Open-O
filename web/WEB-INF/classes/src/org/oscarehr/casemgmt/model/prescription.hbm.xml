<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="org.oscarehr.casemgmt.model.Prescription" table="prescription">
        <id name="id" column="script_no" unsaved-value="0">
            <generator class="increment"/>
        </id>
		<property name="provider_no"/>
		<property name="demographic_no"/>
		<property name="date_prescribed" type="java.util.Date"/>
		<property name="date_printed" type="java.util.Date"/>
		<property name="dates_reprinted"/>
		<property name="textView"/>
		
		<!-- many-to-one name="drug" 
			class="org.oscarehr.casemgmt.model.Drug"
			column="script_no"
			update="false"
			insert="false"
			lazy="false"
		/ -->
    </class>
</hibernate-mapping>
