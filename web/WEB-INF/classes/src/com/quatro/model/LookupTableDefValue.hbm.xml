<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.quatro.model.LookupTableDefValue" table="APP_LOOKUPTABLE" mutable="false">
		<id name="tableId" unsaved-value="0">
			<generator class="native" />
		</id>
        <property name="tableName" column="TABLE_NAME" /> 
        <property name="description" /> 
        <property name="active" column="ACTIVEYN" /> 
		<property name="tree" column="ISTREE" />
        <property name="treeCodeLength" column="TREECODE_LENGTH" />
        <property name="moduleId"/>
        <property name="moduleName"  formula="(select t.description from v_lookup_table t where t.prefix='MOD' and t.CODE=moduleId)"/> 
	</class>
</hibernate-mapping>
